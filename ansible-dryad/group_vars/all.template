postgresql:
  version: 9.5
  repo: 'deb http://apt.postgresql.org/pub/repos/apt/ precise-pgdg main'
  install_development_headers: true

  user: postgres
  group: postgres

  locale: 'en_US'
  encoding: 'UTF-8'

  config:
    shared_buffers: 24MB
    work_mem: 1MB

    checkpoint_segments: 3
    checkpoint_completion_target: 0.5

    effective_cache_size: 128MB

dryad:
  user: vagrant
  user_home: /home/vagrant
  java:
